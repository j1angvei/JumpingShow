<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <ToggleButton
        android:id="@+id/tb_auto"
        style="@style/AppTheme.Button.Toggle"
        android:background="@drawable/bg_toggle_repeat"
        android:checked="true" />

    <ImageButton
        tools:ignore="ContentDescription"
        style="@style/AppTheme.Button.Image"
        android:id="@+id/ib_jump"
        android:src="@drawable/ic_trending_up_black_24dp" />

    <ImageButton
        tools:ignore="ContentDescription"
        android:id="@+id/ib_to_config"
        style="@style/AppTheme.Button.Image"
        android:src="@drawable/ic_settings_black_24dp" />

    <ImageButton
        tools:ignore="ContentDescription"
        style="@style/AppTheme.Button.Image"
        android:id="@+id/ib_exit"
        android:src="@drawable/ic_exit_to_app_black_24dp" />
</merge>
